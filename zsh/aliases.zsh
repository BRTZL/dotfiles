# Aliases

# Directory shortcuts
alias work="cd ~/Documents/workspace/"
alias play="cd ~/Documents/playground/"

# Xcode
alias openXcode="open ios/*.xcworkspace"

# Yarn
alias yad="yarn add --dev"

# SVG
alias svgcleanall="find . -type f -name \"*.svg\" | sudo xargs -I file svgcleaner file file"

# Task Master
alias tm='task-master'
alias taskmaster='task-master'

# Tailscale
alias tailscale="/Applications/Tailscale.app/Contents/MacOS/Tailscale"

# Claude
alias ccask='claude -p'
